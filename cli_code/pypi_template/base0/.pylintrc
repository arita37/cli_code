[MESSAGES CONTROL]
# General
# :W0232: Class has no __init__ method
# :W0611, W0612, W0613, W0614: Unused import
# :C0111,C0115,C0116: Missing docstring
# :W0403: Relative import %r, should be %r
# :W0107: Unnecessary pass statement
# :C0321: Multiple statements
# :C1801: Don't use len to determinate emptyC0412
# :C0412: Import packages are not grouped
# :W0511: Disable To do warnings
#  Style
# :C0303, C0330: Trailed whitespaces, wrong continued indentation (Fix with IDE formatter)
# :C0304: Final newline missing
# :C0411: Third party should be placed before ...
# :R1705: Unnecessary else after return
# :R0903: Too few public methods (let use single classes)
# Noisy / Buggy
# :R0801: Similar lines

disable=W0232,W0611, W0612, W0613, W0614, C0111, C0115, C0116, W0403, W0107, C0321, C1801, C0412, W0511, E1101, E0611,
         R0901, R0201, C0303, C0330, C0304, C0411, R1705, R0903, R0801,C0321,C0103,E1221,C0301,E1305,E1121,C0302,C0330,
         R0913,W0703,E0012,R1716,R0914,W0707

[SIMILARITIES]
ignore-comments=yes
ignore-docstrings=yes
ignore-imports=yes
min-similarity-lines=4

[BASIC]
good-names=i,j,k,n,ex,Run,_,id,pk,setUp,assertJson
const-rgx=(([A-Z][A-Z0-9_]*)|([a-z_][a-z0-9_]*)|(__.*__))$
